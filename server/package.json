{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "build:server": "rimraf ./dist && babel ./src -d dist",
    "server:dev": "npm run build:server && cross-env NODE_ENV=development nodemon dist/index.js",
    "server:pro": "npm run build:server && cross-env NODE_ENV=production nodemon dist/index.js",
    "cleint:dev": "npm run start --prefix client",
    "app:dev": "concurrently \"npm run server:dev\" \"npm run cleint:dev\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.3.3",
    "@graphql-tools/load": "^7.4.1",
    "@graphql-tools/merge": "^8.2.1",
    "@graphql-tools/schema": "^8.3.1",
    "apollo-server": "^3.5.0",
    "bcrypt": "^5.0.1",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^16.0.1",
    "graphql-tools": "^8.2.0",
    "graphql-ws": "^5.5.5",
    "i": "^0.3.7",
    "npm": "^8.1.4",
    "pg": "^8.7.1",
    "rimraf": "^3.0.2",
    "sequelize": "^6.9.0",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "nodemon": "^2.0.15"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  }
}
